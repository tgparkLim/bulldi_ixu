                android:layout_gravity="center"
                android:background="@drawable/control_bg">

            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center">
                <openstack.util.CheckBoxImageView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/Sleeping_3"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:clickable="true"
                    app:checked_img="@drawable/control_on"
                    app:default_img="@drawable/control_off"
                    android:layout_gravity="center"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="3.75dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:background="@drawable/control_bg">

            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center">
                <openstack.util.CheckBoxImageView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/Sleeping_4"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:clickable="true"
                    app:checked_img="@drawable/control_on"
