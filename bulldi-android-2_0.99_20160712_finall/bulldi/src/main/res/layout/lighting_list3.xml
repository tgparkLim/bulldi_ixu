<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:foo="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/White">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <openstack.util.TextViewPlus
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/lighting_item_2"
            foo:customFont="Roboto-Regular.ttf"
            android:textSize="18dp"
            android:textStyle="bold"
            android:layout_marginLeft="11.25dp"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            />
        <openstack.util.TextViewPlus
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="right"
            android:text="@string/lighting_item_2_guide"
            foo:customFont="Roboto-Regular.ttf"
            android:textSize="13.5dp"
            android:layout_marginRight="11.25dp"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            />
    </LinearLayout>
    <TableLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="5dp"
        android:layout_marginLeft="11.25dp"
        >
        <TableRow>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center">
                <openstack.util.CheckBoxImageView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/Sleeping_1"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:clickable="true"
                    app:checked_img="@drawable/control_on"
                    app:default_img="@drawable/control_off"
                    android:layout_gravity="center"

                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="3.75dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:background="@drawable/control_bg">

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center">
                <openstack.util.CheckBoxImageView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/Sleeping_2"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:clickable="true"
                    app:checked_img="@drawable/control_on"
                    app:default_img="@drawable/control_off"
                    android:layout_gravity="center"
                    />
